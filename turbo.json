{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "env": ["ENCORE_BACKEND_CLIENT_API_KEY"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "lint": {
      "outputs": []
    },
    "check": {
      "dependsOn": ["lint"],
      "outputs": []
    },
    "format:check": {
      "outputs": []
    },
    "format:write": {
      "outputs": []
    }
  }
}
